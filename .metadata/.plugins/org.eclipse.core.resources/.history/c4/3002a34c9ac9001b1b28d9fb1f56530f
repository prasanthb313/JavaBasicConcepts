package com.oopsconcepts.pages;


class University
{
	public void percentage(int count)
	{
		if(count<=10) 
		{
			System.out.println("pass percentage is 50 percentage");
		}
		else
		{
			System.out.println("pass percenatge is less");
		}
	}
}

class Andhra extends University
{
	public void percentage(int count)
	{
		if(count<=10) 
		{
			System.out.println("pass percentage is 50 percentage");
		}
		else
		{
			System.out.println("pass percenatge is less");
		}
	}
}

