package com.oopsconcepts.pages;

class HirearchicalInherit
{
      public void mainMethod()
      {
    	  System.out.println("Executing the main method");
      }
}

class FisrstClass extends HirearchicalInherit
{
	public void firstMethod()
	{
		System.out.println("executing the first method");
	}
}

class SecondClass extends HirearchicalInherit
{
	public void secondMethod()
	{
		System.out.println("executing the third method");
	}
}



public class UserClass extends SecondClass
{
	public static void main(String[] args) {
		UserClass user=new UserClass();
		user.mainMethod();
		user.secondMethod();
	}
	
}