package com.oopsconcepts.pages;

public class Encapsulation {

	private int a=20;
	private String name="prasanth";
	
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
		 
}
