package com.oopsconcepts.pages;

public class Composition {

	public void example1() {
		System.out.println("this is for composition ");
	}
	public void example2() {
		System.out.println("this is for composition 2");
	}	
	class Composition1 {
		public static void main(String[] args) {
			Composition ref=new Composition();
			ref.example1();
			ref.example2();
		}
}

}
