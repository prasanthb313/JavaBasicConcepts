package com.oopsconcepts.pages;

class FirstClass 	//super class
{
    public void firstMethod() 
    {
    	System.out.println("Executing first method");
    }
}

class SecondClass extends FirstClass
{
	 public void secondMethod() 
	 {
	    System.out.println("Executing second method");
	 }
}

class ThirdClass extends SecondClass
{
	 public void thirdMethod() 
	 {
	    System.out.println("Executing third method");
	 }
}

public class MainClass extends ThirdClass
{
	public static void main(String[] args) 
	{
		MainClass main=new MainClass();
		main.firstMethod();
		main.secondMethod();
		main.thirdMethod();
	}
}













