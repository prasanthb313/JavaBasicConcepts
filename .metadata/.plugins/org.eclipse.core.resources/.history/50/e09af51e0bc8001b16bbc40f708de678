package com.oopsconcepts.pages;

public class Bolcks {

    {
		System.out.println("---------This is first block----------");
	}
    {
		System.out.println("------second block----------");
	}
	static 
	{
		System.out.println("------second block----------");
	}
	
	public static void main(String[] args) {
		Bolcks ref=new Bolcks();
	}
}
