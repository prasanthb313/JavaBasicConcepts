package com.oopsconcepts.pages;

public class MainClass
{
	public void login()
	{
		System.out.println("executing the login Method");
	}
}

class FirstClass extends MainClass
{
	public void firstMethod()
	{
		System.out.println("executing the first method");
	}
}

class SecondClass extends MainClass
{
	public void thirdMethod()
	{
		System.out.println("executing the third method");
	}
}
