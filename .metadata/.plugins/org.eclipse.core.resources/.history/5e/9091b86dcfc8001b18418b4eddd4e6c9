package com.oopsconcepts.pages;

class Launch 
{
	public void navigate()
	{
		System.out.println("method is used to navigate to browser");
	}
}
class Login extends Launch
{
	public void login()
	{
		System.out.println("Login into the application");
	}
}
class Logout  extends Login
{
	public void logout()
	{
		System.out.println("logging out from the application");
	}
}

class UserLogin extends Logout
{
	public static void main(String[] args) {
		UserLogin login=new UserLogin();
		login.navigate();
		login.login();
		login.logout();
	}
}