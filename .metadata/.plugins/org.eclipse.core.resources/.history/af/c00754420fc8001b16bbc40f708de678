package com.oopsconcepts.pages;

class SingleLevelInheritance {

	int number=10;
	String name="andhrapradesh";	
	
	public void exmple() {
		System.out.println("This methos is for printing examples");
	}
}
class MainClass extends SingleLevelInheritance{
	public static void main(String[] args) {
		MainClass main=new MainClass();
		
		System.out.println("---printing the variables--------");		
		System.out.println(main.number+"   "+main.name);
		main.exmple();
		
	}
}

